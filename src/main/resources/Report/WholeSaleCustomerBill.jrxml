	<queryString language="SQL">
		<![CDATA[SELECT
         fo.orderId,
         f.fishId,
         f.name AS fishName,
         fo.qty AS fishQty,
         f.wholeSalePrice AS fishPrice,
         (fo.qty * f.wholeSalePrice) AS fishTotalAmount
     FROM fish_order fo
     JOIN fish f ON fo.fishId = f.fishId
     WHERE fo.orderId = $P{OrderId}

     UNION

     SELECT
         ao.orderId,
         a.accId,
         a.name AS accName,
         ao.qty AS accQty,
         a.wholeSalePrice AS accPrice,
         (ao.qty * a.wholeSalePrice) AS accTotalAmount
     FROM accessories_order ao
     JOIN accessories a ON ao.accId = a.accId
     WHERE ao.orderId = $P{OrderId};]]>
	</queryString>
